{
  "name": "openapi-editor",
  "version": "1.0.0",
  "description": "A clicky editor for OpenAPI specs",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha",
    "build": "webpack",
    "dev": "webpack && webpack-dev-server --content-base build --hot true",
    "thought": "thought run -a",
    "prethoughtcheck": "thought --version || npm -g install thought",
    "thoughtcheck": "thought check-engines",
    "version": "npm run thought",
    "preversion": "npm run thoughtcheck"
  },
  "author": "Nils Knappmeier",
  "license": "ISC",
  "dependencies": {
    "@types/redux-localstorage": "^1.0.0",
    "jquery": "^3.1.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-localstorage": "^1.0.0-rc5",
    "redux-storage": "^4.1.2",
    "redux-storage-engine-localstorage": "^1.1.4",
    "semantic-ui-react": "^0.64.2",
    "zepto": "^1.2.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/chai-as-promised": "0.0.29",
    "@types/jquery": "^2.0.37",
    "@types/mocha": "^2.2.35",
    "@types/react": "^15.0.0",
    "@types/react-dom": "^0.14.19",
    "@types/react-redux": "^4.4.35",
    "@types/redux": "^3.6.0",
    "@types/redux-storage": "^4.0.7",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "live-server": "^1.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "source-map-loader": "^0.1.5",
    "typescript": "^2.1.4",
    "watchify": "^3.8.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/nknapp/openapi-editor.git"
  },
  "bugs": {
    "url": "https://gitlab.com/nknapp/openapi-editor/issues"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "zepto": "global:Zepto"
  },
  "homepage": "https://gitlab.com/nknapp/openapi-editor#README"
}
